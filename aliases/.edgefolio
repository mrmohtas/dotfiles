# Directors
alias app='~/apps/edgefolio/source/webapps'
alias api='~/apps/edgefolio/source/api'
alias conf='~/apps/edgefolio/configuration'

# Setup
alias vup='conf && vagrant up && rm site.retry'
alias vre='conf && vagrant reload && rm site.retry'
alias vssh='conf && vagrant ssh'
alias ansifu='conf && ansible-playbook site.yml -i servers/edgefolio/local'
alias ansiup='ansifu --tags=update'
alias ansisk='ansifu  --skip-tags=webapp,update'